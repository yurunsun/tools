/var/log/voice/voserver.log
{
        rotate 100
	copytruncate
	dateext
	minsize 1M
        daily
        missingok
        notifempty
        delaycompress
        compress
        postrotate
                reload rsyslog >/dev/null 2>&1 || true
        endscript
}

